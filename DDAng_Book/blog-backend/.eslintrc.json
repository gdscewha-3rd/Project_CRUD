{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended","prettier"],
    "globals":{
        "Atomics":"readOnly",
        "SharedArrayBuffer":"readOnly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType":"module"
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off"
    }
}
